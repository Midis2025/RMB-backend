{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "property"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "addressLine": {
      "type": "string"
    },
    "suburb": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "propertyType": {
      "type": "enumeration",
      "enum": [
        "house",
        "apartment",
        "land",
        "rural"
      ]
    },
    "price": {
      "type": "biginteger"
    },
    "formattedPrice": {
      "type": "string"
    },
    "bedrooms": {
      "type": "integer"
    },
    "bathrooms": {
      "type": "integer"
    },
    "carSpaces": {
      "type": "integer"
    },
    "landSize": {
      "type": "string"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "galleryImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "featured": {
      "type": "boolean"
    },
    "premium": {
      "type": "boolean"
    },
    "description": {
      "type": "richtext"
    },
    "publishOrder": {
      "type": "integer"
    },
    "listingStatus": {
      "type": "enumeration",
      "enum": [
        "active",
        "under_offer",
        "sold"
      ]
    }
  }
}
